{
    "agent": {
      "metrics_collection_interval": 60,
      "run_as_user": "cwagent"
    },
    "metrics": {
      "metrics_collected": {
        "mem": {
          "measurement": [
            "mem_used_percent"
          ]
        },
        "disk": {
          "measurement": [
            "used_percent"
          ],
          "resources": [
            "/"
          ]
        }
      },
      "append_dimensions": {
        "InstanceId": "${aws:InstanceId}"
      }
    }
  }  